{
  "name": "kaytum",
  "version": "0.0.1",
  "description": "Opinionated, Redux abstraction with built-in immutability, normalization, and selectors support.",
  "engines": {
    "node": ">= 10.13"
  },
  "author": "Lucas Lira Gomes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kayak/kaytum.git"
  },
  "bugs": {
    "url": "https://github.com/kayak/kaytum/issues"
  },
  "homepage": "https://kayak.github.io/kaytum",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "test:coverage": "jest --verbose --coverage",
    "docs:watch": "docz dev",
    "docs:build": "docz build",
    "docs:deploy": "npm run docs:build && gh-pages -d public --repo git@github.com:kayak/kaytum.git --",
    "changelog": "conventional-changelog",
    "coveralls": "coveralls",
    "build": "npm run lint && npm run test && tsc --build tsconfig.release.json",
    "release": "np",
    "release:docs": "npm run docs:deploy",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.15",
    "@types/lodash-es": "4.17.3",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "axios": "0.19.0",
    "commitizen": "4.0.3",
    "conventional-changelog-cli": "2.0.23",
    "coveralls": "3.0.5",
    "cz-conventional-changelog": "3.0.2",
    "docz": "1.2.0",
    "docz-theme-default": "1.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "6.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.13.6",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "gh-pages": "2.0.1",
    "jest": "24.8.0",
    "jest-extended": "0.11.2",
    "jsdom-global": "3.0.2",
    "np": "5.0.3",
    "peer-deps-externals-webpack-plugin": "1.0.4",
    "rimraf": "2.6.3",
    "source-map-loader": "0.2.4",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "url-composer": "1.15.0",
    "webpack": "4.37.0",
    "webpack-cli": "3.3.6",
    "webpack-merge": "4.2.1"
  },
  "peerDependencies": {
    "@types/lodash-es": ">=4.17.3",
    "@types/react": ">=16.8.23",
    "@types/react-dom": ">=16.8.5",
    "@types/react-redux": ">=7.1.1",
    "@types/redux": ">=3.6.0",
    "axios": ">=0.19.0",
    "immer": ">=3.1.3",
    "lodash-es": ">=4.17.15",
    "normalizr": ">=3.4.0",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "react-redux": ">=7.1.0",
    "reselect": ">=4.0.0",
    "tslib": ">=1.10.0",
    "url-composer": "1.15.0"
  },
  "dependencies": {
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "@types/react-redux": "7.1.1",
    "@types/redux": "3.6.0",
    "immer": "3.1.3",
    "lodash-es": "4.17.15",
    "normalizr": "3.4.0",
    "react": "16.8.6",
    "react-redux": "7.1.0",
    "react-dom": "16.8.6",
    "reselect": "4.0.0",
    "tslib": "1.10.0"
  },
  "hooks": {
    "pre-commit": "npm test && npm run lint",
    "pre-push": "npm test && npm run lint",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
