{
  "name": "react-resux",
  "version": "0.3.2",
  "description": "Opinionated, Redux abstraction with built-in immutability, normalization, and selectors support.",
  "engines": {
    "node": ">= 10.13"
  },
  "author": "Lucas Lira Gomes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kayak/react-resux.git"
  },
  "bugs": {
    "url": "https://github.com/kayak/react-resux/issues"
  },
  "homepage": "https://kayak.github.io/react-resux",
  "license": "Apache-2.0",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "clean": "rimraf dist js_coverage .docz .rpt2_cache",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "test:coverage": "jest --verbose --coverage",
    "docs:watch": "npm run typedoc:build && docz dev",
    "docs:build": "npm run typedoc:build && docz build",
    "docs:deploy": "npm run docs:build && gh-pages -d .docz/dist --repo git@github.com:kayak/react-resux.git --",
    "changelog": "conventional-changelog",
    "coveralls": "coveralls",
    "build": "npm run lint && npm run test && rollup -c",
    "size": "npm run build && size-limit",
    "typedoc:pre": "rimraf docs/api",
    "typedoc": "typedoc",
    "typedoc:post": "npm run typedoc:post:readme && npm run typedoc:post:subscriber && npm run typedoc:post:model && npm run typedoc:post:modeloptions",
    "typedoc:post:readme": "concat -o docs/api/README.md docs/headers/readme.mdx docs/api/README.md",
    "typedoc:post:model": "concat -o docs/api/classes/model.md docs/headers/modelClass.mdx docs/api/classes/model.md",
    "typedoc:post:modeloptions": "concat -o docs/api/interfaces/modeloptions.md docs/headers/modelOptionsInterface.mdx docs/api/interfaces/modeloptions.md",
    "typedoc:post:subscriber": "concat -o docs/api/classes/subscriber.md docs/headers/subscriberClass.mdx docs/api/classes/subscriber.md",
    "typedoc:build": "npm run typedoc:pre && npm run typedoc && npm run typedoc:post",
    "release": "release-it",
    "release:docs": "npm run docs:deploy",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@release-it/conventional-changelog": "1.1.0",
    "@size-limit/preset-small-lib": "2.1.5",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.18",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "@typescript-eslint/typescript-estree": "2.3.1",
    "commitizen": "4.0.3",
    "concat": "1.0.3",
    "conventional-changelog-cli": "2.0.23",
    "coveralls": "3.0.5",
    "cz-conventional-changelog": "3.0.2",
    "docz": "1.2.0",
    "docz-theme-default": "1.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "gh-pages": "2.0.1",
    "husky": "3.0.2",
    "jest": "24.9.0",
    "jest-extended": "0.11.2",
    "jsdom-global": "3.0.2",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "1.5.3",
    "release-it": "12.3.4",
    "rimraf": "3.0.0",
    "rollup": "1.20.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.1",
    "rollup-plugin-typescript2": "0.24.0",
    "source-map-loader": "0.2.4",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "typedoc": "0.15.0",
    "typedoc-plugin-markdown": "2.2.1",
    "typescript": "3.6.2"
  },
  "peerDependencies": {
    "immer": ">=3.1.3",
    "lodash": ">=4.17.15",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "reselect": ">=4.0.0",
    "react-redux": ">=4.4.9",
    "redux-saga": ">=1.0.5",
    "tslib": ">=1.10.0"
  },
  "dependencies": {
    "@types/lodash": "~4.14.138",
    "@types/react": "~16.8.23",
    "@types/react-dom": "~16.8.5",
    "@types/redux": "~3.6.0",
    "immer": "~3.1.3",
    "lodash": "~4.17.15",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "react-redux": ">=4.4.9",
    "redux-saga": "~1.0.5",
    "reselect": "~4.0.0",
    "tslib": "~1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint",
      "pre-push": "npm test && npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
